<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
        <!-- 驱动jar包，mssql使用jtds-1.2.jar,mysql使用mysql-connector-java-5.0.8-bin.jar -->
        <!--<classPathEntry location="mysql-connector-java-5.1.25-bin.jar" />-->

        <context id="FreeCMS_Tables" targetRuntime="MyBatis3">
                <!--<plugin type="org.mybatis.generator.plugins.PaginationPlugin"/>-->
            <!--<plugin type="org.mybatis.generator.plugins.CaseInsensitiveLikePlugin"></plugin>-->
            <!--<plugin type="org.mybatis.generator.plugins.SerializablePlugin"></plugin>-->
            <plugin type="com.systp.pagePulgin.PaginationPlugin"/>
                <commentGenerator>
            <property name="suppressDate" value="true"/>
            <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
                 <!-- mysql配置 -->
                <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://180.76.113.232:3306/systp" userId="root"
                        password="root">
                </jdbcConnection>

                <javaTypeResolver>
                        <property name="forceBigDecimals" value="false" />
                </javaTypeResolver>
                
                <!-- model生成文件的存放位置 -->
                <javaModelGenerator targetPackage="com.meice.servicemeice.entity"
                        targetProject="C:\Users\tp\Desktop\mybatis-generator-core-1.3.2\lib\src">
                        <property name="enableSubPackages" value="true" />
                        <property name="trimStrings" value="true" />
                </javaModelGenerator>
                
                <!-- sqlmap生成文件的存放位置 -->
                <sqlMapGenerator targetPackage="com.meice.servicemeice.dao.mapper"
                        targetProject="C:\Users\tp\Desktop\mybatis-generator-core-1.3.2\lib\src">
                        <property name="enableSubPackages" value="true" />
                </sqlMapGenerator>

                <!-- dao生成文件的存放位置 -->
                <javaClientGenerator type="XMLMAPPER"
                        targetPackage="com.meice.servicemeice.dao"
                        targetProject="C:\Users\tp\Desktop\mybatis-generator-core-1.3.2\lib\src">
                        <property name="enableSubPackages" value="true" />
                </javaClientGenerator>

                <!-- 要生成的表 -->
                <table tableName="lable"  domainObjectName="Lable"></table>
                <table tableName="article"  domainObjectName="Article"></table>


        </context>
</generatorConfiguration>